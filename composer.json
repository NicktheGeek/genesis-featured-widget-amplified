    {
      "name": "Nick Croft",
      "homepage": "https://designsbynickthegeek.com/",
      "role": "Developer"
    },
    {
      "name": "Gary Jones",
      "homepage": "https://gamajo.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/NicktheGeek/genesis-featured-widget-amplified/issues",
