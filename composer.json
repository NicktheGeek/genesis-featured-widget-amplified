  "keywords": [
    "plugin",
    "gwfa",
    "Genesis",
    "widget"
  ],
  "type": "wordpress-plugin",
  "license": "GPL-2.0+",
  "homepage": "https://github.com/NicktheGeek/genesis-featured-widget-amplified",
